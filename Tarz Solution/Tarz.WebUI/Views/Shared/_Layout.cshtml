    <script>
        $(document).ready(function () {

            $('#frmSubscribe').submit(function (e) {
                e.preventDefault();

                if ($(e.currentTarget).valid() != true)
                    return;

                const formData = new FormData(e.formData);
                $.ajax({
                  url:'@Url.Action("Subscribe")',
                    type: 'POST'
                    data: formData,
                    contentType: false,
                    processData: false,
                    success: function (response) {

                    },
                   error: function (response) {

                    }

                });

            }).validate({
              errorElement:'span'
            });
        });
